{
  "name": "build-md",
  "version": "0.4.0",
  "description": "Markdown builder for JS/TS",
  "license": "MIT",
  "author": "MatÄ›j Chalk <matejchalk@gmail.com>",
  "keywords": [
    "markdown",
    "md",
    "builder"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/matejchalk/build-md.git"
  },
  "homepage": "https://github.com/matejchalk/build-md#readme",
  "bugs": {
    "url": "https://github.com/matejchalk/build-md/issues"
  },
  "type": "module",
  "files": [
    "dist/"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prepare": "husky",
    "commit": "git-cz",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "docs": "typedoc",
    "release": "release-it",
    "release:dry-run": "release-it --dry-run"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@types/node": "^20.14.9",
    "@vitest/coverage-v8": "^1.6.0",
    "commitizen": "^4.3.0",
    "commitlint-plugin-tense": "^1.0.2",
    "husky": "^9.0.11",
    "inquirer": "^9.3.5",
    "release-it": "^17.5.0",
    "semantic-release": "^24.0.0",
    "tsup": "^8.0.2",
    "typedoc": "^0.26.3",
    "typedoc-material-theme": "^1.1.0",
    "typedoc-plugin-extras": "^3.1.0",
    "typedoc-plugin-missing-exports": "^3.0.0",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
